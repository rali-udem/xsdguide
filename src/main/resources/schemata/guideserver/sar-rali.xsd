        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="B">
        <xs:annotation>
          <xs:documentation>Submachine gun or machine pistol</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="C">
