  <xs:element name="PersonHeightMeasure" type="sarrali:LengthType">
    <xs:annotation><xs:documentation>The vertical height of a person.</xs:documentation></xs:annotation>
  </xs:element>

  <xs:complexType name="LengthType">
    <xs:annotation><xs:documentation>A length.</xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element ref="sarrali:MeasurePointValue"/>
        <xs:element ref="sarrali:MeasureRangeValue"/>
      </xs:choice>
      <xs:element minOccurs="1" ref="sarrali:LengthUnitCode"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="PersonWeightMeasure">
    <xs:annotation><xs:documentation>The weight of a person.</xs:documentation></xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:MeasurePointValue"/>
        <xs:element ref="sarrali:MassUnitCode"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="MeasurePointValue" type="xs:decimal"/>

  <xs:element name="PersonName">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="sarrali:PersonGivenName"/>
        <xs:element minOccurs="0" ref="sarrali:PersonSurName"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="PersonGivenName" type="xs:NCName">
    <xs:annotation><xs:documentation>A person's first name.</xs:documentation></xs:annotation>
  </xs:element>
  <xs:element name="PersonSurName" type="xs:NCName">
    <xs:annotation><xs:documentation>A person's last name.</xs:documentation></xs:annotation>
  </xs:element>
  <xs:element name="PersonRaceCode" type="sarrali:RACCodeSimpleType"/>

  <xs:simpleType name="RACCodeSimpleType">
