  <xs:element name="PersonBuildText" type="xs:string">
    <xs:annotation>
      <xs:documentation>A physique or shape of a person.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="PersonFacialHairText" type="xs:string">
    <xs:annotation>
      <xs:documentation>A kind of facial hair of a person.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="EntityLocation">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="1" ref="sarrali:LocationDescriptionText"/>
        <xs:element minOccurs="0" ref="sarrali:LocationAddress"/>
        <xs:element minOccurs="0" ref="sarrali:LocationPrecisionText"/>
      </xs:all>
      <xs:attribute name="id" use="required" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="LocationDescriptionText" type="xs:string"/>

  <xs:element name="LocationAddress">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:StructuredAddress"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="StructuredAddress">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:LocationStreet"/>
        <xs:element ref="sarrali:LocationCityName"/>
        <xs:element ref="sarrali:LocationStateName"/>
        <xs:element ref="sarrali:LocationPostalCode"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="LocationStreet">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:StreetNumberText"/>
        <xs:element ref="sarrali:StreetName"/>
        <xs:element ref="sarrali:StreetCategoryText"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="StreetNumberText" type="xs:positiveInteger"/>
  <xs:element name="StreetName" type="xs:NCName"/>
  <xs:element name="StreetCategoryText" type="xs:NCName"/>
  <xs:element name="LocationCityName" type="xs:NCName"/>
  <xs:element name="LocationStateName" type="xs:NCName"/>
  <xs:element name="LocationPostalCode" type="xs:string"/>
  <xs:element name="LocationPrecisionText" type="xs:string"/>

  <xs:element name="EntityFirearm">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:Firearm"/>
      </xs:sequence>
      <xs:attribute name="id" use="required" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Firearm">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="sarrali:ItemDescriptionText"/>
        <xs:element minOccurs="0" ref="sarrali:ItemColorDescriptionText"/>
        <xs:element minOccurs="0" ref="sarrali:ItemLengthMeasure"/>
        <xs:element minOccurs="0" ref="sarrali:FirearmCategoryCode"/>
        <xs:element minOccurs="0" ref="sarrali:FirearmFinishCode"/>
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="ItemDescriptionText" type="xs:string"/>

  <xs:element name="ItemColorDescriptionText" type="xs:NCName"/>

  <xs:element name="ItemLengthMeasure" type="sarrali:LengthType"/>

  <xs:element name="FirearmCategoryCode" type="sarrali:TYPCodeSimpleType"/>

