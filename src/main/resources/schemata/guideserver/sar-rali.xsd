<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  elementFormDefault="qualified"
  xmlns:sarrali      ="http://rali.iro.umontreal.ca/sarrali"  
  targetNamespace="http://rali.iro.umontreal.ca/sarrali" >

  <xs:element name="SuspiciousActivityReport">
    <xs:annotation>
      <xs:documentation>A structure that describes a SAR Report </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:Metadata" />
        <xs:element ref="sarrali:Data"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Metadata">
    <xs:annotation>
      <xs:documentation>A structure that describes Metadata about a related SAR</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:UniqueId"/>
        <xs:element ref="sarrali:Title"/>
        <xs:element ref="sarrali:SubmissionSystem"/>
        <xs:element ref="sarrali:Author"/>
        <xs:element ref="sarrali:CreationDateTime"/>
        <xs:element name="DisseminationCriteria" type="sarrali:DisseminationCriteriaType"/>
        <xs:element ref="sarrali:RelatedSarList" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="UniqueId" type="xs:ID">
    <xs:annotation>
      <xs:documentation>An id unique to this report</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Title" type="xs:string">
    <xs:annotation>
      <xs:documentation>A short report title</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="SubmissionSystem">
    <xs:annotation>
      <xs:documentation>Element that uniquely identifies an organization and a system where date originated, was submitted from, or is being sent.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:OrganizationName"/>
        <xs:element ref="sarrali:Identifier"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="OrganizationName" type="xs:string">
    <xs:annotation><xs:documentation>The organization's name.</xs:documentation></xs:annotation>
  </xs:element>

  <xs:element name="Identifier" type="xs:NCName">
    <xs:annotation>
      <xs:documentation>A name identifying an organization.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Author">
    <xs:annotation>
      <xs:documentation>Contact information for the system owner. Includes a person and organization to contact and their phone number and email address.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
         <xs:element ref="sarrali:AuthorPerson"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="AuthorPerson">
    <xs:annotation>
      <xs:documentation>The person who authors the report.</xs:documentation>
    </xs:annotation>
    
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="sarrali:FirstName"/>
        <xs:element ref="sarrali:LastName"/>
        <xs:element ref="sarrali:OrganizationName"/>
        <xs:element ref="sarrali:TelephoneNumber" minOccurs="0" maxOccurs="3"/>
        <xs:element ref="sarrali:EMailAddress"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="FirstName" type="xs:NCName">
    <xs:annotation>
        <xs:documentation>The person's first name.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="LastName" type="xs:NCName">
    <xs:annotation>
        <xs:documentation>The person's last name.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="TelephoneNumber" type="xs:string" />

  <xs:element name="EMailAddress">
    <xs:annotation>
      <xs:documentation>An electronic mailing address by which a person or organization may be contacted.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:pattern value=".+@.+\..+"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>

  <xs:element name="CreationDateTime" type="xs:dateTime">
    <xs:annotation>
      <xs:documentation>Date and time the report was created.</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:simpleType name="DisseminationCriteriaType">
    <xs:annotation>
      <xs:documentation>This information is provided by the data owner to indicate who is allowed to see the information.  For publish it provides information to the service provider about what can be returned as the result of a search. For search or get results, it provides information to the user so they know the criteria for this information.  Note that this is different from the data sensitivity included at the message level.  The values and their meanings for dissemination criteria are shown below.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="white">
        <xs:annotation>
          <xs:documentation>all users are allowed access</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="gray">
        <xs:annotation>
          <xs:documentation>the data owner has full access; all other users are provided pointer (i.e. who to contact and how) information only</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="black">
        <xs:annotation>
          <xs:documentation>the data owner has full access; all other users receive no information and no indication that there was a hit when performing searches</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="RelatedSarList">
    <xs:annotation>
      <xs:documentation>A structure that can contain a list of related SARs</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sarrali:RelatedSar"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="RelatedSar">
    <xs:annotation>
      <xs:documentation>The id of a related SAR.</xs:documentation>
    </xs:annotation>

    <xs:complexType>
      <xs:attribute name="id" use="required" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="Data">
    <xs:annotation>
      <xs:documentation>The primary information contained in a SAR.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sarrali:EntityPerson"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sarrali:EntityLocation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sarrali:EntityFirearm"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sarrali:Activity"/>
        <xs:element minOccurs="0" ref="sarrali:Associations"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="EntityPerson">
    <xs:annotation>
      <xs:documentation>An element for a human being.</xs:documentation>
    </xs:annotation>

    <xs:complexType>
      <xs:all>
        <xs:element ref="sarrali:PersonAgeMeasure" minOccurs="0"/>
        <xs:element ref="sarrali:PersonEyeColorCode" minOccurs="0"/>
        <xs:element ref="sarrali:PersonHairColorCode" minOccurs="0"/>
        <xs:element ref="sarrali:PersonHeightMeasure" minOccurs="0"/>
        <xs:element ref="sarrali:PersonWeightMeasure" minOccurs="0"/>
        <xs:element ref="sarrali:PersonName" minOccurs="0"/>
        <xs:element ref="sarrali:PersonRaceCode" minOccurs="0"/>
        <xs:element ref="sarrali:PersonSexCode" minOccurs="1"/>
        <xs:element ref="sarrali:PersonBuildText" minOccurs="0"/>
        <xs:element ref="sarrali:PersonFacialHairText" minOccurs="0"/>
      </xs:all>
      <xs:attribute name="id" use="required" type="xs:ID"/>
    </xs:complexType>
  </xs:element>

  <xs:element name="PersonAgeMeasure">
    <xs:annotation>
      <xs:documentation>The age of a person.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="sarrali:MeasureRangeValue">
            <xs:annotation>
              <xs:documentation>A range of value, from min value to max value.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element ref="sarrali:MeasurePointValue">
            <xs:annotation>
              <xs:documentation>A specific value.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:choice>
        <xs:element ref="sarrali:TimeUnitCode"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="TimeUnitCode" type="sarrali:TimeCodeSimpleType"/>

  <xs:element name="PersonEyeColorCode" type="sarrali:EYECodeSimpleType"/>

